        }
    ],
    "require": {
        "php": "~7.4.0|~7.2.0|~7.3.0|~7.4.0",
        "magento/framework": "100.1.*|101.0.*|102.0.*|103.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.13",
        "squizlabs/php_codesniffer": "3.3.1",
        "phpmd/phpmd": "@stable",
        "php-coveralls/php-coveralls": "~2.1.0"
    },
    "suggest": {
        "renatocason/magento2-module-mq-mysql": "~1.2.3",
        "renatocason/magento2-module-mq-amqp": "~1.1.3"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0"
    ],
